html(lang="en")
  head
    title CloudPalette
    link(rel='stylesheet', type='text/css', href='/css/style.css')
    script(type='text/javascript', src='http://code.jquery.com/jquery-1.6.2.min.js')
    script(type='text/javascript', src='/js/util/canvasUtil.js')
    script(type='text/javascript', src='/js/canvasDemo.js')    
  body
    #index-wrapper
      img#splash-logo(src='/assets/images/CloudPaletteLogo.png')
      img#splash-dash(src='/assets/images/splashpagedash.png')
      #create
        p#create-text
          | CloudPalette is an online image editor. Test it out!
        a#create-pic(href='/editor')
          img(src='/assets/images/create.png')
      #code
        p#code-text
          | Check out the code on github.
        a#code-pic(href='https://github.com/hodavidhara/CloudPalette')
          img(src='/assets/images/code.png')